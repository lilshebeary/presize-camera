{
  "expo": {
    "name": "presize-camera",
    "slug": "presize-camera",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FB2A5C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beary.presize-camera"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoBlue.png",
        "backgroundColor": "#212121"
      },
      "package": "com.beary.presizecamera"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
